generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model NmProject {
  id          String   @id
  siteUrl     String
  status      String   // "created" | "synced" | "paused"
  lastEventAt DateTime?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
